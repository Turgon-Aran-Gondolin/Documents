Cell["1.4", "Subsection"],

Cell["Consider the follwing Volterra type integral equation", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"y", 
    RowBox[{"(", "x", ")"}]}], "+", " ", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "x"], 
    RowBox[{
     SubscriptBox["J", "0"], 
     RowBox[{"(", 
      RowBox[{"x", "-", "t"}], ")"}], "y", 
     RowBox[{"(", "t", ")"}], 
     RowBox[{"\[DifferentialD]", "t"}]}]}]}], "=", 
  RowBox[{
   RowBox[{"sin", 
    RowBox[{"(", "x", ")"}]}], " ", "+", 
   RowBox[{
    SubscriptBox["J", "0"], 
    RowBox[{"(", "x", ")"}]}]}]}]], "Input"],

Cell[TextData[{
 "with exact solution y(x)=",
 Cell[BoxData[
  SubscriptBox["J", "0"]]],
 "(x)."
}], "Text"],

Cell["\<\
We construct a high-precision solution with absolute uniform error \
\>", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"sol", "=", 
  RowBox[{
   RowBox[{"NISolve", "[", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"y", "[", "x", "]"}], "+", " ", 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "x"], 
        RowBox[{
         RowBox[{"BesselJ", "[", 
          RowBox[{"0", ",", 
           RowBox[{"x", "-", "t"}]}], "]"}], 
         RowBox[{"y", "[", "t", "]"}], 
         RowBox[{"\[DifferentialD]", "t"}]}]}]}], "\[Equal]", 
      RowBox[{
       RowBox[{"Sin", "[", "x", "]"}], " ", "+", 
       RowBox[{"BesselJ", "[", 
        RowBox[{"0", ",", "x"}], "]"}]}]}], ",", "y", ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "0", ",", "1"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"PrecisionGoal", "\[Rule]", "14"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Error", "\[Rule]", "Absolute"}], ",", "\[IndentingNewLine]", 
