}], "Section"],

Cell[CellGroupData[{

Cell["4.1", "Subsection"],

Cell["Consider the difference-differential boundary  problem", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"y", "''"}], 
    RowBox[{"(", "x", ")"}]}], "=", 
   RowBox[{
    RowBox[{"q", 
     RowBox[{"(", "x", ")"}], "y", 
     RowBox[{"(", 
      RowBox[{"x", "-", "1"}], ")"}]}], "+", 
    RowBox[{
     RowBox[{"f", "''"}], 
     RowBox[{"(", "x", ")"}]}]}]}], ",", " ", 
  RowBox[{"x\[Epsilon]", "[", 
   RowBox[{
    RowBox[{"-", "\[Infinity]"}], ",", "2"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y", 
   RowBox[{"(", 
    RowBox[{"-", "\[Infinity]"}], ")"}]}], "=", "0"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"y", "'"}], 
   RowBox[{"(", 
    RowBox[{"-", "\[Infinity]"}], ")"}]}], "=", "0"}]}], "Input"],

Cell["which is equivalent to the following integral equation", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"y", 
   RowBox[{"(", "x", ")"}]}], "=", 
  RowBox[{
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{"-", "\[Infinity]"}], 
     RowBox[{"x", "-", "1"}]], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"x", "-", "t", "-", "1"}], ")"}], "q", 
     RowBox[{"(", 
      RowBox[{"t", "+", "1"}], ")"}], 
     RowBox[{"y", "[", "t", "]"}], 
     RowBox[{"\[DifferentialD]", "t"}]}]}], "+", 
   RowBox[{"f", 
    RowBox[{"(", "x", ")"}]}]}]}]], "Input"],

Cell[TextData[{
 "We solve this problem for ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"q", 
    RowBox[{"(", "x", ")"}]}], "=", 
   SuperscriptBox["e", 
    RowBox[{"x", "/", "2"}]]}]]],
